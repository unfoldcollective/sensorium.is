			<h3 class="partnerlist-title">{{page.header.supportersTitle}}</h3>
			<div class="partnerlist">
				{% for supporter in page.header.supporters %}
					<a class="partnerlist-logo{{ supporter.major ? ' partnerlist-logo--major' : '' }}" href="{{supporter.url}}" title="{{supporter.name}}" target="_blank" >
						<img title="{{supporter.name}}" class="partnerlist-logo__img"src="{{ page.media[supporter.logo].url }}" alt="{{supporter.name}}">
					</a>
				{% endfor %}
			</div>

			<h3 class="partnerlist-title">{{page.header.partnersTitle}}</h3>
			<div class="partnerlist">
				{% for partner in page.header.partners %}
					<a class="partnerlist-logo{{ partner.major ? ' partnerlist-logo--major' : '' }}" href="{{partner.url}}" title="{{partner.name}}" target="_blank" >
						<img title="{{partner.name}}" class="partnerlist-logo__img"src="{{ page.media[partner.logo].url }}" alt="{{partner.name}}">
					</a>
				{% endfor %}
			</div>

			<h3 class="partnerlist-title">{{page.header.edupartnersTitle}}</h3>
			<div class="partnerlist">
				{% for partner in page.header.edupartners %}
					<a class="partnerlist-logo{{ partner.major ? ' partnerlist-logo--major' : '' }}" href="{{partner.url}}" title="{{partner.name}}" target="_blank" >
						<img title="{{partner.name}}" class="partnerlist-logo__img"src="{{ page.media[partner.logo].url }}" alt="{{partner.name}}">
					</a>
				{% endfor %}
			</div>

			<h3 class="partnerlist-title">{{page.header.mainMediaPartnersTitle}}</h3>
			<div class="partnerlist">
				{% for mediaPartner in page.header.mainMediaPartners %}
					<a class="partnerlist-logo{{ mediaPartner.major ? ' partnerlist-logo--major' : '' }}" href="{{mediaPartner.url}}" title="{{mediaPartner.name}}" target="_blank" >
						<img title="{{mediaPartner.name}}" class="partnerlist-logo__img"src="{{ page.media[mediaPartner.logo].url }}" alt="{{mediaPartner.name}}">
					</a>
				{% endfor %}
			</div>

			<h3 class="partnerlist-title">{{page.header.mediaPartnersTitle}}</h3>
			<div class="partnerlist">
				{% for mediaPartner in page.header.mediaPartners %}
					<a class="partnerlist-logo{{ mediaPartner.major ? ' partnerlist-logo--major' : '' }}" href="{{mediaPartner.url}}" title="{{mediaPartner.name}}" target="_blank" >
						<img title="{{mediaPartner.name}}" class="partnerlist-logo__img"src="{{ page.media[mediaPartner.logo].url }}" alt="{{mediaPartner.name}}">
					</a>
				{% endfor %}
			</div>		
		<p>{{ page.header.supportSentence }}</p>	
	</div>
</div>







