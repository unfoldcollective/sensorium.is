<div class="grid-x grid-padding-x section">
	<div class="medium-3 cell">
		<h2>{{ page.title }}</h2>
	</div>
	<div class="medium-9 cell">
		<h3 class="partnerlist-title">{{page.header.mainPartnersTitle}}</h3>
			<div class="partnerlist">
				{% for mainPartner in page.header.mainPartners %}
					<a class="partnerlist-logo{{ mainPartner.major ? ' partnerlist-logo--major' : '' }}" href="{{mainPartner.url}}" title="{{mainPartner.name}}" target="_blank" >
						<img title="{{mainPartner.name}}" class="partnerlist-logo__img" src="{{ page.media[mainPartner.logo].url }}" alt="{{mainPartner.name}}">
					</a>
				{% endfor %}
			</div>

			{# <h3 class="partnerlist-title">{{page.header.sponsorsTitle}}</h3>
