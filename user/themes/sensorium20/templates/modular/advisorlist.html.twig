<div class="grid-x grid-padding-x advisors section">
	<div class="small-6 medium-3 cell">
		<h2>{{ page.title }}</h2>
	</div>
	<div class="small-6 medium-9 cell grid-x grid-padding-x">
		{% for advisor in page.header.advisors %}
			<div class="medium-4 cell">
				<p>{{ advisor.name }}</p>
				<p>{{ advisor.title }}</p>
			</div>
		{% endfor %}
	</div>
</div>